-frames+={hide,"kind(unit)&&main(^.*?lib/.*?\\.c$)"}
-frames+={hide,"kind(unit)&&main(^.*?kernel/.*?\\.c$)"}
-frames+={hide,"kind(unit)&&main(^.*?soc/.*?\\.c$)"}
-frames+={hide,"kind(unit)&&main(^.*?modules/.*?\\.c$)"}
-frames+={hide,"kind(unit)&&main(^.*subsys/.*?\\.c$)"}
-frames+={hide,"kind(unit)&&main(^.*misc/.*?\\.c$)"}
-frames+={hide,"kind(unit)&&main(^.*include/zephyr/.*?\\.c$)"}
-frames+={hide,"kind(unit)&&main(^.*?build/zephyr/.*?\\.c$)"}
-frames+={hide,"kind(unit)&&main(^.*twister/.*$)"}
-frames+={hide,"kind(unit)&&main(^.*zephyr/.*$)"}
-frames+={hide,"kind(unit)&&main(^.*generated.*$)"}
